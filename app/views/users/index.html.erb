<h1>Listing users</h1>

<%= link_to 'New User', new_user_path %>

<table id="users">
  <thead>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th></th>
    
  </tr>
  </thead>


  <% @users.each do |u| %>
      <%= render partial: "/users/user", locals: {user: u} %>
      <%= render partial: "/users/user_edit", locals: {user: u} %>
  <% end %>


</table>

<br>


<p class="pagination" hidden>
  <%= link_to_next_page(@users, 'Next Page', :remote => true) %>
</p>